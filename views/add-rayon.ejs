              <div class="form-group">
                <label for="nameR">Nom Rayon :</label>
                <input type="text" class="form-control"   name="nameR" required>
              </div>
              <button type="submit" class="btn btn-primary">Submit</button>
            </form>
          </div>
      <!-- /col-lg-12 -->
    </div>
    <!-- /row -->
  </section>
  <!-- /wrapper -->
</section>
